#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	double h = 0.6;
	int n = 40, m = 10,a=0;
	double* z = new double[n];
	double* Z = new double[n];
	z[0] = -0.50;
	z[1] = 1.80;
	cout << z[0] << " " << z[1] << " ";
	for (int i=2;i<=n;i++)
	{
		if (z[i-1] >= 0)
		{
				z[i] = z[i-2] * cos(z[i-1] - z[i-2] + 1);
		}
		else
		{
			{
				z[i] = 2 * cos(z[i-1] + z[i-2]);
			}
		}
	}
	for (int i = 0;i <= n;i++)
	{
		Z[i] = abs(z[i]);
	}
	for (int j = 1;j <= m;j++)
	{
		for (int i = 0;i <= n;i++)
		{
			if ((Z[i] >= ((j - 1) * h)) && (Z[i] < (j * h)))
			{
				a++;
			}
		}
		cout << " " << a << " ";
		a = 0;
	}
	return 0;
	system("pause");
}
